<div class="container">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>">
      <%= value %>
    </div>
  <% end %>

  <div class="page-header">
    <h1><%= yield :page_header %></h1>
  </div>
<p class="notice"><%= notice %></p>
       <p class="alert"><%= alert %></p>

  <%= yield %>
</div>